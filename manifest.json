{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "version": "0.13.0",
  "default_locale": "ko",
  "icons": {
    "16": "img/16.png",
    "22": "img/22.png",
    "24": "img/24.png",
    "32": "img/32.png",
    "48": "img/48.png",
    "128": "img/128-crap.png"
  },
  "action": {
    "default_icon": "img/16.png",
    "default_title": "kwcag a11y inspector"
  },
  "background": {
    "service_worker": "js/service-worker.js"
  },
  "options_ui": {
    "page": "option.html",
    "browser_style": true
  },
  "permissions": ["contextMenus", "tabs", "scripting", "storage"],
  "host_permissions": ["https://*/*", "http://*/*"]
}
