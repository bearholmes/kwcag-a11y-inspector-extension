{
  "manifest_version": 2,
  "name": "kwcag a11y inspector",
  "description": "kwcag 2.1.3 test",
  "version": "0.12.0",
  "icons": {
    "16": "img/16.png",
    "22": "img/22.png",
    "24": "img/24.png",
    "32": "img/32.png",
    "48": "img/48.png"
  },
  "browser_action": {
    "default_icon": "img/16.png",
    "default_title": "kwcag a11y inspector"
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": true
  },
  "options_ui": {
    "page": "option.html",
    "chrome_style": true
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*",
    "contextMenus"
  ]
}
