{
  "name": "kwcag-a11y-inspector",
  "version": "0.13.0",
  "description": "KWCAG 2.1 Accessibility Inspector - Chrome Extension for measuring operable criteria",
  "scripts": {
    "format": "prettier . --write",
    "build": "babel src --out-dir js --source-maps",
    "build:prod": "babel src --out-dir js --source-maps --minified --no-comments",
    "watch": "babel src --out-dir js --source-maps --watch",
    "clean": "rm -rf js/*.js js/*.map",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "author": "bearholmes <bearholmes@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bearholmes/kwcag-a11y-inspector-extension"
  },
  "keywords": [
    "accessibility",
    "a11y",
    "kwcag",
    "wcag",
    "chrome-extension",
    "inspector",
    "contrast-checker"
  ],
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@types/chrome": "^0.0.248",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^2.8.4"
  }
}
